package com.company;
import java.util.*;
public class pkhv {

    public static void main(String[] args) {
        ArrayList<String> ls=new ArrayList<>();
        ls.add("red");
        ls.add("pink");
        ls.add("blue");
        ls.add("black");
        ls.add("violet");
        System.out.println("original array list " + ls);
        System.out.println("print using index of an element  ");
        int no_of_elements=ls.size();
        for(int index=0;index<no_of_elements;index++){
            System.out.println(ls.get(index));
        }
    }
