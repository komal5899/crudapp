Advanced java exercise:  1


package com.company;
import java.util.*;
public class Main {

    public static void main(String[] args) {
             List<String> ls=new ArrayList<>();
             ls.add("red");
             ls.add("pink");
             ls.add("blue");
             ls.add("black");
             ls.add("violet");
             System.out.println(ls);
             ls.remove(2);
             System.out.println("After removing third element " + ls);
    }
}
